{
  "version": "1.0.0",
  "scripts": {
    "start": "export NODE_ENV=local && cd ./envs/local/ && webpack-dev-server",
    "build": "webpack --progress --config envs/dev/webpack.config.js",
    "dev:build": "export NODE_ENV=development && webpack --progress --config envs/dev/webpack.config.js",
    "dev:deploy": "SLS_DEBUG=* serverless client deploy --stage development -z '.js$|.css$' --region ap-northeast-1 && echo 'SUCCESS! open browser to http://<bucket-name>.s3-website-<region>.amazonaws.com'",
    "test": "NODE_ENV=test jest",
    "coverage": "NODE_ENV=test jest --coverage",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:eslint": "eslint .",
    "lint:css": "stylelint ./**/*.css"
  },
  "jest": {
		"moduleFileExtensions": ["ts", "tsx", "js"],
		"transform": {
			"^.+\\.tsx?$": "<rootDir>/bin/jestpreprocessor.js"
		},
    "testRegex": "(/__tests__/.*|(\\.)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/build/",
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.0.4-rc.0",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^18.1.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-test-renderer": "^15.4.2",
    "serverless-client-s3": "git+https://github.com/kyunghoon/serverless-client-s3.git#serverless_1.0_beta_compatibility",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "typescript": "^2.1.6",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.52",
    "@types/react": "^15.0.8",
    "@types/react-addons-update": "^0.14.16",
    "@types/react-dom": "^0.14.23",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router": "^3.0.3",
    "@types/recompose": "^0.20.4",
    "apollo-client": "^0.8.1",
    "babel-polyfill": "^6.22.0",
    "classnames": "^2.2.5",
    "graphql-tag": "^1.2.3",
    "lodash": "^4.17.4",
    "milligram-cssnext": "^1.0.1",
    "rambda": "^0.5.2",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-apollo": "^0.9.0",
    "react-dom": "^15.4.2",
    "react-geomicons": "^2.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.2",
    "rebass": "^0.3.3",
    "recompose": "^0.22.0",
    "reflexbox": "^2.2.3"
  }
}
